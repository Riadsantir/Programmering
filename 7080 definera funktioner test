def berakna_genomsnitt(tal_lista):
    # Funktionen beräknar genomsnittet av talen i listan.
    if not tal_lista:
        return None  # Returnera None om listan är tom
    else:
        return sum(tal_lista) / len(tal_lista)

def klassificera_genomsnitt(genomsnitt):
    # Funktionen klassificerar genomsnittet i olika intervall.
    if genomsnitt is None:
        return "Listan är tom"
    elif genomsnitt < 50:
        return "Under medel"
    elif genomsnitt < 75:
        return "Medel"
    else:
        return "Över medel"

def main():
    # Huvudprogrammet som använder de tidigare definierade funktionerna.

    # Exempeldata
    tal_lista = [65, 72, 89, 45, 60]

    # Beräkna genomsnittet
    genomsnitt = berakna_genomsnitt(tal_lista)

    # Skriv ut genomsnittet och dess klassificering
    print("Genomsnittet är:", genomsnitt)
    print("Klassificering:", klassificera_genomsnitt(genomsnitt))

if __name__ == "__main__":
    main()
